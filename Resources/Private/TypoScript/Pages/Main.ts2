main < page{
	bodyTag.attributes.class = 'Page Page--Main'
	body{
		templatePath = 'resource://Sfi.Sfi/Private/Templates/Pages/Main.html'
		presentationMenu = Menu{
			templatePath = 'resource://Sfi.Sfi/Private/Templates/TypoScriptObjects/PresentationMenu.html'
			entryLevel = 1
			maximumLevels = 1
		}
		breadcrumbMenu>
	}
}

root.main {
	condition = ${q(node).is('[instanceof Sfi.Sfi:PageMain]')}
	renderPath = '/main'
}